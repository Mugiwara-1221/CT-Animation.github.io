{
  "$schema": "https://json.schemastore.org/staticwebapp.database.config.json",
  "dataSource": "cosmosDB",
  "connectionStringSetting": "COSMOS_DB_CONNECTION",
  "collections": {
    "Products": {
      "allowedOperations": ["read", "create", "update", "delete"]
    },
    "Orders": {
      "allowedOperations": ["read", "create"]
    },
  "nationFallback": {
    "exclude": ["/api/*"]
  }
  }
}
